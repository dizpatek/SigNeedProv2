generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Document {
  id             String   @id @default(cuid())
  name           String
  originalUrl    String
  signedUrl      String?
  status         String   @default("PENDING")
  signatureZones String?
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
  isDeleted      Boolean  @default(false)
}
